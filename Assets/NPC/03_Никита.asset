  m_Name: "03_\u041D\u0438\u043A\u0438\u0442\u0430"
  m_EditorClassIdentifier: 
  text: "[\u041F\u0420\u041E\u0424\u0418\u041B\u042C \u041F\u0415\u0420\u0421\u041E\u041D\u0410\u0416\u0410] 
    \u0418\u043C\u044F \u2013 \u041D\u0438\u043A\u0438\u0442\u0430 \u0421\u0435\u0440\u0451\u0436\u043A\u0438\u043D\L\u0412\u043E\u0437\u0440\u0430\u0441\u0442
    - 23 \u0433\u043E\u0434\u0430\L\u0411\u0438\u043E\u0433\u0440\u0430\u0444\u0438\u044F:
    \u0420\u043E\u0434\u0438\u043B\u0441\u044F \u0432 \u043E\u0431\u0435\u0441\u043F\u0435\u0447\u0435\u043D\u043D\u043E\u0439
    \u0441\u0435\u043C\u044C\u0435, \u0440\u043E\u0434\u0438\u0442\u0435\u043B\u0438
    \u0432\u0441\u0435\u0433\u0434\u0430 \u0435\u043C\u0443 \u0432 \u043F\u043E\u043F\u0443
    \u0434\u0443\u043B\u0438, \u043F\u0440\u043E\u0431\u043B\u0435\u043C \u0438 \u0431\u0435\u0434
    \u043E\u043D \u043D\u0435 \u0437\u043D\u0430\u043B, \u043D\u0438\u043A\u043E\u0433\u0434\u0430
    \u0441\u0435\u0431\u0435 \u043D\u0438 \u0432 \u0447\u0451\u043C \u043D\u0435
    \u043E\u0442\u043A\u0430\u0437\u044B\u0432\u0430\u043B. \u041A\u043E\u0433\u0434\u0430
    \u0440\u043E\u0434\u0438\u0442\u0435\u043B\u0438 \u043F\u043E\u043D\u044F\u043B\u0438,
    \u0447\u0442\u043E \u0438\u0437\u0431\u0430\u043B\u043E\u0432\u0430\u043B\u0438
    \u0435\u0433\u043E, \u0431\u044B\u043B\u043E \u0441\u043B\u0438\u0448\u043A\u043E\u043C
    \u043F\u043E\u0437\u0434\u043D\u043E. \u0412\u044B\u0440\u043E\u0441 \u043E\u043D
    \u043A\u0440\u0430\u0439\u043D\u0435 \u0430\u0437\u0430\u0440\u0442\u043D\u044B\u043C,
    \u0434\u043E\u043B\u0433\u043E\u0435 \u0432\u0440\u0435\u043C\u044F \u0441\u0441\u043E\u0440\u0438\u043B\u0441\u044F
    \u0441 \u0440\u043E\u0434\u0438\u0442\u0435\u043B\u044F\u043C\u0438, \u043D\u043E
    \u043A\u043E\u0433\u0434\u0430 \u043E\u043D \u0434\u0435\u043F\u043D\u0443\u043B
    \u0432 \u043A\u0430\u0437\u0438\u043A \u043E\u0433\u0440\u043E\u043C\u043D\u0443\u044E
    \u0441\u0443\u043C\u043C\u0443, \u0442\u0435\u0440\u043F\u0435\u043D\u0438\u0435
    \u043E\u0442\u0446\u0430 \u043B\u043E\u043F\u043D\u0443\u043B\u043E \u0438 \u043E\u043D
    \u0432\u044B\u0433\u043D\u0443\u043B \u0435\u0433\u043E \u0438\u0437 \u0434\u043E\u043C\u0430,
    \u0447\u0442\u043E\u0431\u044B \u0442\u043E\u0442 \u043F\u043E\u043D\u044F\u043B,
    \u043A\u0430\u043A\u043E\u0432\u043E \u044D\u0442\u043E, \u0440\u0430\u0431\u043E\u0442\u0430\u0442\u044C.
    \u041D\u0438\u043A\u0438\u0442\u0430 \u043F\u043E\u043F\u0440\u043E\u0431\u043E\u0432\u0430\u043B
    \u043C\u043D\u043E\u0436\u0435\u0441\u0442\u0432\u043E \u0434\u043E\u043B\u0436\u043D\u043E\u0441\u0442\u0435\u0439,
    \u0441 \u0437\u0430\u0432\u043E\u0434\u0430 \u0435\u0433\u043E \u0432\u044B\u0433\u043D\u0430\u043B\u0438
    \u0437\u0430 \u0442\u043E, \u0447\u0442\u043E \u043E\u043D \u0438\u0433\u0440\u0430\u043B
    \u0432 \u043A\u0430\u0440\u0442\u044B, \u043F\u043E\u043A\u0430 \u0435\u0433\u043E
    \u043A\u043E\u043B\u043B\u0435\u0433\u0443 \u043D\u0430\u043A\u0440\u0443\u0447\u0438\u0432\u0430\u043B\u043E
    \u043D\u0430 \u0441\u0442\u0430\u043D\u043E\u043A, \u0438\u0437 \u043C\u0430\u0433\u0430\u0437\u0438\u043D\u0430
    \u0435\u0433\u043E \u0432\u044B\u0433\u043D\u0430\u043B\u0438 \u043F\u043E\u0441\u043B\u0435
    \u0442\u043E\u0433\u043E, \u043A\u0430\u043A \u043E\u043D \u043F\u0440\u043E\u0438\u0433\u0440\u0430\u043B
    \u0432\u0435\u0441\u044C \u043F\u0440\u0438\u043B\u0430\u0432\u043E\u043A \u043F\u043E\u043A\u0443\u043F\u0430\u0442\u0435\u043B\u044E,
    \u0432 \u0438\u0442\u043E\u0433\u0435 \u043E\u043D \u043F\u0440\u0438\u0448\u0451\u043B
    \u0440\u0430\u0431\u043E\u0442\u0430\u0442\u044C \u0433\u0440\u0443\u0437\u0447\u0438\u043A\u043E\u043C
    \u043D\u0430 \u0433\u043E\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043D\u043D\u044B\u0439
    \u0433\u0440\u0443\u0437\u043E\u0432\u043E\u0439 \u0434\u0438\u0440\u0438\u0436\u0430\u0431\u043B\u044C.
    \u041F\u0440\u043E\u0444\u0435\u0441\u0441\u0438\u044F - \u0433\u0440\u0443\u0437\u0447\u0438\u043A\L\u0425\u0430\u0440\u0430\u043A\u0442\u0435\u0440
    \u2013 \u043A\u0440\u0430\u0439\u043D\u0435 \u0430\u0437\u0430\u0440\u0442\u043D\u044B\u0439
    \u0438 \u044F\u0437\u0432\u0438\u0442\u0435\u043B\u044C\u043D\u044B\u0439. \u041F\u043E\u0441\u0442\u043E\u044F\u043D\u043D\u043E
    \u0443\u0441\u0442\u0440\u0430\u0438\u0432\u0430\u0435\u0442 \u0430\u0440\u043C-\u0440\u0435\u0441\u0442\u043B\u0438\u043D\u0433
    \u0441\u043E \u0441\u0442\u0430\u0432\u043A\u0430\u043C\u0438 \u0441 \u0410\u043D\u043D\u043E\u0439.
    \u041A\u0440\u0430\u0439\u043D\u0435 \u0441\u0430\u0440\u043A\u0430\u0441\u0442\u0438\u0447\u0435\u043D
    \u0438 \u0432\u044B\u0441\u043E\u043A\u043E\u043C\u0435\u0440\u0435\u043D, \u043F\u0440\u0438
    \u044D\u0442\u043E\u043C \u0441\u043E\u0432\u0435\u0440\u0448\u0435\u043D\u043D\u043E
    \u043D\u0435 \u0443\u0432\u0435\u0440\u0435\u043D \u0432 \u0441\u0435\u0431\u0435.
    \u041D\u0430\u0440\u0446\u0438\u0441\u0441, \u044D\u0433\u043E\u0438\u0441\u0442,
    \u043D\u0435\u043F\u0440\u0438\u044F\u0442\u043D\u044B\u0439 \u0442\u0438\u043F.
    \u0421\u0442\u0440\u0430\u0434\u0430\u0435\u0442 \u041E\u041A\u0420 (\u0432\u0435\u0440\u043D\u0435\u0435
    \u0431\u0443\u0434\u0435\u0442 \u0441\u043A\u0430\u0437\u0430\u0442\u044C \u043D\u0430\u0441\u043B\u0430\u0436\u0434\u0430\u0435\u0442\u0441\u044F)
    \u0438 \u043F\u043E\u0441\u0442\u043E\u044F\u043D\u043D\u043E \u0432\u0441\u0435\u043C
    \u043F\u0440\u0435\u0434\u044A\u044F\u0432\u043B\u044F\u0435\u0442 \u0437\u0430
    \u043C\u0430\u043B\u0435\u0439\u0448\u0438\u0439 \u0431\u0435\u0441\u043F\u043E\u0440\u044F\u0434\u043E\u043A.
    \u0418\u043C\u0435\u0435\u0442 \u043E\u0441\u0442\u0440\u0443\u044E \u043F\u043E\u0442\u0440\u0435\u0431\u043D\u043E\u0441\u0442\u044C
    \u0432 \u043A\u043E\u043D\u0444\u043B\u0438\u043A\u0442\u0430\u0445, \u043C\u043E\u0436\u0435\u0442
    \u0432\u043E\u043E\u0431\u0449\u0435 \u0431\u0435\u0437 \u043F\u0440\u0438\u0447\u0438\u043D\u044B
    \u043D\u0430\u0435\u0445\u0430\u0442\u044C \u043D\u0430 \u0447\u0435\u043B\u043E\u0432\u0435\u043A\u0430.
    \u041D\u0438\u043A\u043E\u0433\u0434\u0430 \u043D\u0435 \u0437\u0434\u043E\u0440\u043E\u0432\u0430\u0435\u0442\u0441\u044F
    \u0438 \u0436\u0430\u043B\u0435\u0435\u0442 \u0434\u0430\u0436\u0435 \u0441\u043B\u043E\u0432,
    \u043F\u043E\u044D\u0442\u043E\u043C\u0443 \u043C\u043E\u0436\u0435\u0442 \u0431\u044B\u0442\u044C
    \u043D\u0435\u043C\u043D\u043E\u0433\u043E\u0441\u043B\u043E\u0432\u0435\u043D.
    \u0412\u043D\u0435\u0448\u043D\u043E\u0441\u0442\u044C - \L\u041F\u043E\u0447\u0435\u043C\u0443
    \u043D\u0430\u0445\u043E\u0434\u0438\u0442\u0441\u044F \u043D\u0430 \u0434\u0438\u0440\u0438\u0436\u0430\u0431\u043B\u0435
    \u2013 \u0440\u0430\u0431\u043E\u0442\u0430\u0435\u0442 \u0433\u0440\u0443\u0437\u0447\u0438\u043A\u043E\u043C\L\u0421\u0432\u0435\u0440\u0445\u0437\u0430\u0434\u0430\u0447\u0430
    (\u0447\u0442\u043E \u043E\u043D \u0445\u043E\u0447\u0435\u0442 \u0431\u043E\u043B\u044C\u0448\u0435
    \u0432\u0441\u0435\u0433\u043E) \u2013 \u0437\u0430\u0440\u0430\u0431\u043E\u0442\u0430\u0442\u044C
    \u043C\u043D\u043E\u0433\u043E \u0434\u0435\u043D\u0435\u0433 \u0438 \u043E\u0442\u043A\u0440\u044B\u0442\u044C
    \u0441\u0432\u043E\u0439 \u0441\u043E\u0431\u0441\u0442\u0432\u0435\u043D\u043D\u044B\u0439
    \u043A\u0430\u0437\u0438\u043A. "
  npcName: "\u041D\u0438\u043A\u0438\u0442\u0430 \u0421\u0435\u0440\u0451\u0436\u043A\u0438\u043D"
