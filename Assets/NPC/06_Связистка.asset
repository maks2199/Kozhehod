  m_Name: "06_\u0421\u0432\u044F\u0437\u0438\u0441\u0442\u043A\u0430"
  m_EditorClassIdentifier: 
  text: "[\u041F\u0420\u041E\u0424\u0418\u041B\u042C \u041F\u0415\u0420\u0421\u041E\u041D\u0410\u0416\u0410] 
    \u0418\u043C\u044F: \u0418\u0434\u0430 \u041A\u0440\u0430\u043B\u0438\u0447\L\u0412\u043E\u0437\u0440\u0430\u0441\u0442:
    19 \u043B\u0435\u0442\L\u0411\u0438\u043E\u0433\u0440\u0430\u0444\u0438\u044F:\L\u0420\u043E\u0434\u0438\u0432\u0448\u0438\u0441\u044C
    \u0432 \u043D\u0435\u043B\u0435\u0433\u0430\u043B\u044C\u043D\u043E\u043C \u0446\u044B\u0433\u0430\u043D\u0441\u043A\u043E\u043C
    \u043A\u0432\u0430\u0440\u0442\u0430\u043B\u0435 \u0431\u043B\u0438\u0437 \u0414\u0435\u043D\u0434\u0440\u043E\u0441\u0442\u0430\u043D\u0430,
    \u0418\u0434\u0430 \u0440\u043E\u0441\u043B\u0430 \u043F\u043E\u0440\u0430\u0437\u0438\u0442\u0435\u043B\u044C\u043D\u043E
    \u0443\u043C\u043D\u043E\u0439, \u0445\u043E\u0442\u044C \u0438 \u0442\u0440\u0435\u0432\u043E\u0436\u043D\u043E\u0439,
    \u0434\u0435\u0432\u043E\u0447\u043A\u043E\u0439. \u0412 8 \u043B\u0435\u0442
    \u0435\u0439 \u0441\u043A\u0430\u0437\u0430\u043B\u0438, \u0447\u0442\u043E \u043E\u043D\u0430
    \u0432\u044B\u0439\u0434\u0435\u0442 \u0437\u0430\u043C\u0443\u0436 \u0437\u0430
    \u0441\u043E\u0441\u0435\u0434\u0441\u043A\u043E\u0433\u043E \u043C\u0430\u043B\u044C\u0447\u0438\u043A\u0430,
    \u043E\u0434\u043D\u0430\u043A\u043E, \u0435\u0451 \u0441\u043F\u0430\u0441\u043B\u043E
    \u0442\u043E, \u0447\u0442\u043E \u0447\u0435\u0440\u0435\u0437 \u043D\u0435\u0441\u043A\u043E\u043B\u044C\u043A\u043E
    \u0434\u043D\u0435\u0439 \u043F\u043E\u0441\u043B\u0435 \u044D\u0442\u043E\u0433\u043E
    \u043D\u0430\u0447\u0430\u043B\u0441\u044F \u0441\u043D\u043E\u0441 \u043A\u0432\u0430\u0440\u0442\u0430\u043B\u0430.
    \u0414\u0435\u0432\u043E\u0447\u043A\u0430 \u0441\u0431\u0435\u0436\u0430\u043B\u0430
    \u0432 \u0433\u043E\u0440\u043E\u0434, \u0433\u0434\u0435 \u043D\u0430\u0448\u043B\u0430
    \u043F\u0440\u0438\u0441\u0442\u0430\u043D\u0438\u0449\u0435, \u0430 \u0434\u043B\u044F
    \u0432\u0441\u0435\u0445, \u043A\u0442\u043E \u0435\u0451 \u0437\u043D\u0430\u043B,
    \u043E\u043D\u0430 \u0441\u0442\u0430\u043B\u0430 \u043F\u0440\u043E\u043F\u0430\u0432\u0448\u0435\u0439
    \u0431\u0435\u0437 \u0432\u0435\u0441\u0442\u0438, \u0432\u043E\u0437\u043C\u043E\u0436\u043D\u043E
    \u043F\u043E\u0433\u0438\u0431\u0448\u0435\u0439 \u0432 \u0442\u043E\u0442 \u0434\u0435\u043D\u044C
    \u0434\u0435\u0432\u043E\u0447\u043A\u043E\u0439. \u0412 \u0433\u043E\u0440\u043E\u0434\u0435
    \u043E\u043D\u0430 \u0441\u043C\u043E\u0433\u043B\u0430 \u043F\u043E\u0439\u0442\u0438
    \u0432 \u0448\u043A\u043E\u043B\u0443, \u043A\u043E\u0442\u043E\u0440\u0443\u044E
    \u0443\u0441\u043F\u0435\u0448\u043D\u043E \u0437\u0430\u043A\u043E\u043D\u0447\u0438\u043B\u0430,
    \u043F\u043E\u0441\u043B\u0435 \u0447\u0435\u0433\u043E \u0442\u0430\u043A\u0436\u0435
    \u043F\u043E\u043B\u0443\u0447\u0438\u043B\u0430 \u0432\u044B\u0441\u0448\u0435\u0435
    \u043E\u0431\u0440\u0430\u0437\u043E\u0432\u0430\u043D\u0438\u0435 \u0432 \u0441\u0444\u0435\u0440\u0435
    \u0441\u0432\u044F\u0437\u0438. \u0418\u0437-\u0437\u0430 \u0441\u0442\u0440\u0430\u0445\u0430
    \u043F\u0435\u0440\u0435\u0434 \u043F\u0430\u0440\u0430\u0437\u0438\u0442\u0430\u043C\u0438,
    \u043A\u043E\u0442\u043E\u0440\u044B\u0445 \u0432 \u0414\u0435\u043D\u0434\u0440\u043E\u0441\u0442\u0430\u043D\u0435
    \u043B\u0435\u0433\u043A\u043E \u043F\u043E\u0434\u0446\u0435\u043F\u0438\u0442\u044C,
    \u043F\u0435\u0440\u0435\u0435\u0445\u0430\u043B\u0430 \u0432 \u0411\u0443\u0440\u0435\u0433\u0440\u0430\u0434,
    \u0433\u0434\u0435 \u0438 \u043D\u0430\u0442\u043A\u043D\u0443\u043B\u0430\u0441\u044C
    \u043D\u0430 \u0432\u0430\u043A\u0430\u043D\u0441\u0438\u044E \u0441\u0432\u044F\u0437\u0438\u0441\u0442\u0430
    \u043D\u0430 \u0434\u0438\u0440\u0438\u0436\u0430\u0431\u043B\u0435 \"\u0421\u0435\u0432\u0435\u0440\u043E-\u0437\u0430\u043F\u0430\u0434\"\L\u041F\u0440\u043E\u0444\u0435\u0441\u0441\u0438\u044F:
    \u0441\u0432\u044F\u0437\u0438\u0441\u0442\L\u0425\u0430\u0440\u0430\u043A\u0442\u0435\u0440:
    \u041A\u0440\u0430\u0439\u043D\u0435 \u0442\u0440\u0435\u0432\u043E\u0436\u043D\u0430\u044F
    \u0434\u0435\u0432\u0443\u0448\u043A\u0430, \u0431\u043E\u0438\u0442\u0441\u044F
    \u043F\u0430\u0440\u0430\u0437\u0438\u0442\u043E\u0432, \u043E\u0441\u043E\u0431\u0435\u043D\u043D\u043E
    \u043A\u0438\u0448\u0435\u0447\u043D\u044B\u0445, \u0430 \u0442\u0430\u043A\u0436\u0435
    \u0440\u0430\u0437\u043B\u0438\u0447\u043D\u044B\u0445 \u0442\u0440\u043E\u043F\u0438\u0447\u0435\u0441\u043A\u0438\u0445
    \u0438\u043D\u0444\u0435\u043A\u0446\u0438\u0439, \u043A\u043E\u0442\u043E\u0440\u044B\u0445
    \u043F\u043E\u0432\u0438\u0434\u0430\u043B\u0430 \u0432 \u0436\u0438\u0437\u043D\u0438
    \u043C\u043D\u043E\u0433\u043E. \u041A\u0440\u0430\u0439\u043D\u0435 \u0441\u043A\u0440\u043E\u043C\u043D\u0430\u044F
    \u0434\u0435\u0432\u0443\u0448\u043A\u0430, \u0447\u0430\u0441\u0442\u043E \u0431\u044B\u0432\u0430\u0435\u0442
    \u0441\u0442\u0435\u0441\u043D\u0438\u0442\u0435\u043B\u044C\u043D\u043E\u0439
    \u0438 \u043D\u0435 \u0440\u0430\u0437\u0433\u043E\u0432\u043E\u0440\u0447\u0438\u0432\u043E\u0439,
    \u043F\u043E\u0441\u0442\u043E\u044F\u043D\u043D\u043E \u0445\u043E\u0447\u0435\u0442
    \u0441\u043F\u0430\u0442\u044C. \u041D\u0430\u0448\u043B\u0430 \u043E\u0431\u0449\u0438\u0439
    \u044F\u0437\u044B\u043A \u0441\u043E \u0421\u0432\u0435\u0442\u043E\u0439, \u043F\u043E\u0442\u043E\u043C\u0443
    \u0447\u0442\u043E \u043E\u043D\u0438 \u0440\u043E\u0434\u043E\u043C \u0438\u0437
    \u043E\u0434\u043D\u043E\u0433\u043E \u0433\u043E\u0440\u043E\u0434\u0430 \u0438
    \u0432 \u0446\u0435\u043B\u043E\u043C \u0438\u043C \u0435\u0441\u0442\u044C \u043E
    \u0447\u0451\u043C \u043F\u043E\u0433\u043E\u0432\u043E\u0440\u0438\u0442\u044C.
    \u0411\u043E\u0438\u0442\u0441\u044F \u041D\u0438\u043A\u0438\u0442\u044B, \u043D\u043E
    \u0441\u0442\u0435\u0441\u043D\u044F\u0435\u0442\u0441\u044F \u044D\u0442\u043E
    \u043F\u0440\u0438\u0437\u043D\u0430\u0432\u0430\u0442\u044C.\L\u0412\u043D\u0435\u0448\u043D\u043E\u0441\u0442\u044C:
    \u043D\u0435\u0431\u043E\u043B\u044C\u0448\u0430\u044F \u043C\u043E\u043B\u043E\u0434\u0430\u044F
    \u0434\u0435\u0432\u0443\u0448\u043A\u0430 \u0446\u044B\u0433\u0430\u043D\u0441\u043A\u043E\u0439
    \u043D\u0430\u0440\u0443\u0436\u043D\u043E\u0441\u0442\u0438, \u043D\u043E\u0441\u0438\u0442
    \u0437\u043E\u043B\u043E\u0442\u044B\u0435 \u0443\u043A\u0440\u0430\u0448\u0435\u043D\u0438\u044F
    \u0438 \u044F\u0440\u043A\u0443\u044E \u043A\u0440\u0430\u0441\u043D\u043E-\u0431\u0435\u0436\u0435\u0432\u0443\u044E
    \u0440\u0430\u0441\u043F\u0438\u0441\u043D\u0443\u044E \u0440\u0443\u0431\u0430\u0448\u043A\u0443.\L\u041F\u043E\u0447\u0435\u043C\u0443
    \u043D\u0430 \u0434\u0438\u0440\u0438\u0436\u0430\u0431\u043B\u0435: \u0440\u0430\u0431\u043E\u0442\u0430\u0435\u0442
    \u0441\u0432\u044F\u0437\u0438\u0441\u0442\u043E\u043C\L\u0421\u0432\u0435\u0440\u0445\u0437\u0430\u0434\u0430\u0447\u0430:
    \u0432\u044B\u0441\u043F\u0430\u0442\u044C\u0441\u044F (\u043D\u043E \u044D\u0442\u043E
    \u043D\u0435\u0432\u043E\u0437\u043C\u043E\u0436\u043D\u043E \u0438\u0437-\u0437\u0430
    \u0435\u0451 \u0442\u0440\u0435\u0432\u043E\u0436\u043D\u043E\u0441\u0442\u0438)"
  npcName: "\u0418\u0434\u0430 \u041A\u0440\u0430\u043B\u0438\u0447"
  npcProfession: "\u0421\u0432\u044F\u0437\u0438\u0441\u0442"
  npcPortrait: {fileID: 1623028206407414927, guid: c3e681b0dc8ab424b861acb478f3be2f, type: 3}
  aliveSprite: {fileID: -5695220069881681669, guid: c3a4a42d473a34623856e973705a650e, type: 3}
  deadSprite: {fileID: -1149312594620663861, guid: ff6a31607ae3a42149fef5f9b10ffb33, type: 3}
  startPhrazes:
  - "\u0423 \u0442\u0435\u0431\u044F \u043A\u043E\u0433\u0434\u0430-\u043D\u0438\u0431\u0443\u0434\u044C
    \u0431\u044B\u043B\u043E \u043E\u0449\u0443\u0449\u0435\u043D\u0438\u0435, \u0447\u0442\u043E
    \u0442\u044B \u0432\u043E\u0442-\u0432\u043E\u0442 \u0443\u043C\u0440\u0451\u0448\u044C?"
  - "\u041C\u043D\u0435 \u043A\u0430\u0436\u0435\u0442\u0441\u044F, \u0447\u0442\u043E
    \u0447\u0442\u043E-\u0442\u043E \u043D\u0435 \u0442\u0430\u043A. \u0422\u044B
    \u0442\u043E\u0436\u0435 \u044D\u0442\u043E \u0447\u0443\u0432\u0441\u0442\u0432\u0443\u0435\u0448\u044C?"
  - "\u0415\u0441\u043B\u0438 \u0431\u044B \u043D\u0435 \u043C\u043E\u0438 \u0442\u0440\u0430\u043D\u043A\u0432\u0438\u043B\u0438\u0437\u0430\u0442\u043E\u0440\u044B,
    \u044F \u0431\u044B \u0443\u0436\u0435 \u043E\u0442\u043A\u0438\u043D\u0443\u043B\u0430\u0441\u044C"
  - "\u042F \u0449\u0430\u0441 \u0441\u0430\u043B\u044C\u0442\u043E \u0441\u0434\u0435\u043B\u0430\u044E."
  - "\u0423 \u0442\u0435\u0431\u044F \u0435\u0441\u0442\u044C \u043A\u0430\u043A\u0438\u0435-\u0442\u043E
    \u043D\u043E\u0432\u043E\u0441\u0442\u0438? \u0425\u043E\u0440\u043E\u0448\u0438\u0445,
    \u0435\u0441\u0442\u0435\u0441\u0442\u0432\u0435\u043D\u043D\u043E, \u0436\u0434\u0430\u0442\u044C
    \u043D\u0435 \u043F\u0440\u0438\u0445\u043E\u0434\u0438\u0442\u0441\u044F."
